<%= form_for(@client) do |f| %>
  <%= label :client, :first_name,"First name" %>
  <%= text_field(:client, :first_name) %><br>
  <%= f.label :email,"Email" %>
  <%= f.text_field :email %><br>
  <%= f.label :birthday, "Birthday"%>
  <%= f.text_field :birthday%>
  <%= f.file_field :avatar %>
  <button class="button" type="submit">Submit</button>


<% end %>