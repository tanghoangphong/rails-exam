<p> In ID=<%= @client.id %> <span> Name: <%= @client.first_name %> </span> has <%= @client.orders_count %> order: </p>
<table class="table table-striped">
  <tr>
    <th>ID</th>
    <th>Order Name</th>
    <th>created_at</th>
    <th>updated_at</th>
    
  </tr>
 <% @orders.each do |order| %>
  <tr>
    <td><%= order.id %></td>
    <td><%= order.name %></td>
    <td><%= order.created_at %></td>
    <td><%= order.updated_at %></td>
   
  </tr>

<% end %>
</table>